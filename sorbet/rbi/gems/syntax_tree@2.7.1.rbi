  #   ) -> Args
  def on_args_add_block(arguments, block); end

  # :call-seq:
  #   on_args_add_star: (Args arguments, untyped star) -> Args
  def on_args_add_star(arguments, argument); end

  # :call-seq:
  #   on_args_forward: () -> ArgsForward
  def on_args_forward; end

  # :call-seq:
  #   on_args_new: () -> Args
  def on_args_new; end

